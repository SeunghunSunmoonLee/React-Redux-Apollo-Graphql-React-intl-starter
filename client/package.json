{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-client": "^1.9.1",
    "apollo-test-utils": "^0.3.2",
    "auth0-js": "^8.8.0",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.1.1",
    "events": "^1.1.1",
    "false": "0.0.4",
    "firebase": "^4.2.0",
    "graphql": "^0.10.5",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^1.2.1",
    "intl": "^1.2.5",
    "material-ui": "^0.18.7",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "pusher-js": "^4.1.0",
    "react": "^15.6.1",
    "react-apollo": "^1.4.2",
    "react-dom": "^15.6.1",
    "react-intl": "^2.3.0",
    "react-intl-po": "^2.1.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "subscriptions-transport-ws": "^0.7.3"
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "react-intl-cra": "^0.2.7",
    "react-intl-translations-manager": "^5.0.0",
    "react-scripts": "0.9.3"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./public --include-path ./node_modules public/ -o public/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./public --include-path ./node_modules public/ -o public/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "extract:messages": "react-intl-cra 'src/**/*.js' 'output/messages.json'",
    "manage:translations": "./node_modules/.bin/babel-node src/utils/translator.js",
    "extract:pot": "react-intl-po json2pot 'output/messages.json' -o 'output/messages.pot'",
    "extract:po": "react-intl-po po2json 'output/*.po' -c 'id' -m 'output/messages.json' -o 'output/translation.json'"
  },
  "proxy": "http://localhost:5000"
}
